{
  "presets": ["es2015", "react"],
  "plugins": [
    "lodash",
    "transform-object-rest-spread",
    "transform-object-assign",
    "transform-class-properties"
  ],
  "env": {
    "es": {
      "comments": false
    },
    "commonjs": {
      "comments": false,
      "plugins": ["transform-es2015-modules-commonjs"]
    },
    "test": {
      "plugins": ["transform-decorators-legacy"]
    }
  }
}
